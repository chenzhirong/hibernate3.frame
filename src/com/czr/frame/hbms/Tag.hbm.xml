<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.czr.frame.beans.Tag" table="T_TAG">
        <comment>T_TAG</comment>
        <id name="objId" type="java.lang.Long">
            <column name="OBJ_ID" precision="16" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_OBJ_ID</param>
            </generator>
        </id>
         <property name="tagname" type="java.lang.String">
            <column name="TAGNAME" length="">
                <comment>标签名称</comment>
            </column>
        </property>
         <property name="tagcode" type="java.lang.String">
            <column name="TAGCODE" length="">
                <comment>标签代码</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
